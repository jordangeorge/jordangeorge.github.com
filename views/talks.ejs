<% include ./partials/header %>

<div id="content">
  <% if (talks.length > 0){ %>
    <% talks.forEach(function(talk) { %>
      <div class="post">
        <h1>
          <%= talk.name %>
        </h1>
        <p>
          <%= talk.description %>
        </p>
      </div>
    <% }); %>
  <% } else {%>
    <h1 style="text-align: center">No talks to show.</h1>
  <% } %>
</div>

<% include ./partials/footer %>
